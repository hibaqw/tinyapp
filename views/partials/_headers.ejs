<header>
  <nav class="navbar navbar-expand-md navbar-dark bg-success">
    <a class="navbar-brand" href="/urls">TinyApp</a>
    <button
      class="navbar-toggler"
      type="button"
      data-toggle="collapse"
      data-target="#navbarNavAltMarkup"
      aria-controls="navbarNavAltMarkup"
      aria-expanded="false"
      aria-label="Toggle navigation"
    >
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
      <div class="navbar-nav">
        <a class="nav-item nav-link" href="/urls">My URLs</a>
        <a class="nav-item nav-link" href="/urls/new">Create New URL</a>
      </div>
      <%if(!user){ %> 
      <div style="justify-content:flex-end">
        <a class = "login-link" href = "/login" style="margin-left: 800px;">Login</a>
        <a class = "registration-link" href = "/register" style="margin-left:10px">Register</a>
      </div>
      <% } else { %>
        <div style="display: flex; width: 100%; justify-content: flex-end; margin-right: 1.5em;";><h6 style="margin-right: 15px; margin-top: 5px;" >Logged in as: <%= user["email"] %></h6>
        <form action="/logout" method="post"><button type="submit" class="btn btn-outline-dark">Logout</button></form></div>
      <%}%>
    </div>
  </nav>
</header>
